{
	"name": "sokker-db-analytics3",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"deploy": "npm run build && firebase deploy",
		"preview": "vite preview",
		"update:latest": "npx npm-check-updates -u --interactive",
		"db:optimize": "sqlite3 src/DB/transfers.db < scripts/db:optimize.sql",
		"db:json": "node scripts/db:json.js",
		"db": "npm run db:optimize && npm run db:json",
		"db:fletbuffer": "flatc --ts -o scripts/flatbuffer scripts/flatbuffer/transfer.fbs && npx tsx ./scripts/flatbuffer/db:flatbuffer.js",
		"translate:extract": "node scripts/translations:extract.js",
		"translate:uk": "npm run translate:extract && node scripts/translations:translate.js uk"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.8",
		"axios": "^1.9.0",
		"comlink": "^4.4.2",
		"echarts": "^5.6.0",
		"flatbuffers": "^25.2.10",
		"lucide-react": "^0.511.0",
		"mobx": "^6.13.7",
		"mobx-react": "^9.2.0",
		"mobx-state-tree": "^7.0.2",
		"rc-slider": "^11.1.8",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-grid-layout": "^1.5.1",
		"react-range": "^1.10.0",
		"react-resizable": "^3.0.5",
		"react-virtualized-auto-sizer": "^1.0.26",
		"rosetta": "^1.1.0",
		"rxdb": "^16.12.0",
		"tailwindcss": "^4.1.8",
		"wouter": "^3.7.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^22.15.24",
		"@types/pako": "^2.0.3",
		"@types/react": "^19.1.6",
		"@types/react-dom": "^19.1.5",
		"@types/react-grid-layout": "^1.3.5",
		"@types/react-slider": "^1.3.6",
		"@types/sql.js": "^1.4.9",
		"@vitalets/google-translate-api": "^9.2.1",
		"@vitejs/plugin-react-swc": "^3.10.0",
		"better-sqlite3": "^11.10.0",
		"esbuild": "^0.25.5",
		"eslint": "^9.27.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"glob": "^11.0.2",
		"globals": "^16.2.0",
		"pako": "^2.1.0",
		"path": "^0.12.7",
		"sqlite3": "^5.1.7",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.4",
		"typescript": "~5.8.3",
		"vite": "^6.3.5"
	},
	"packageManager": "pnpm@10.11.0"
}
